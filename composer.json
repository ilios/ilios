{
  "name": "ilios/ilios",
  "license": "MIT",
  "type": "project",
  "description": "The \"Ilios Standard Edition\" distribution",
  "version": "3.133.0",
  "require": {
    "php": ">= 8.4",
    "ext-apcu": "*",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "ext-sodium": "*",
    "ext-xmlwriter": "*",
    "ext-zlib": "*",
    "composer/composer": "^2.1",
    "doctrine/doctrine-bundle": "^3.0",
    "doctrine/doctrine-fixtures-bundle": "^4.0",
    "doctrine/doctrine-migrations-bundle": "4.0.0",
    "doctrine/inflector": "^2.0",
    "doctrine/orm": "^3.4",
    "eluceo/ical": "^2.5.1",
    "exercise/htmlpurifier-bundle": "^5.1",
    "firebase/php-jwt": "^6.11.1",
    "flagception/flagception-bundle": "^6.0",
    "ilios/mesh-parser": "^4.0",
    "jaybizzle/crawler-detect": "^1.2",
    "league/flysystem": "^3.0",
    "league/flysystem-aws-s3-v3": "^3.0",
    "liip/monitor-bundle": "^2.6",
    "nelmio/api-doc-bundle": "^5.0",
    "nelmio/cors-bundle": "^2.0",
    "opensearch-project/opensearch-php": "^2.0",
    "pear/archive_tar": "^1.4",
    "psr/log": "^3.0.0",
    "sentry/sentry-symfony": "^5.0",
    "swagger-api/swagger-ui": "^5.1",
    "symfony/amazon-mailer": "7.4.*",
    "symfony/apache-pack": "@stable",
    "symfony/asset": "7.4.*",
    "symfony/console": "7.4.*",
    "symfony/doctrine-messenger": "7.4.*",
    "symfony/dotenv": "7.4.*",
    "symfony/flex": "@stable",
    "symfony/framework-bundle": "7.4.*",
    "symfony/http-client": "7.4.*",
    "symfony/ldap": "7.4.*",
    "symfony/lock": "7.4.*",
    "symfony/mailchimp-mailer": "7.4.*",
    "symfony/mailer": "7.4.*",
    "symfony/mailgun-mailer": "7.4.*",
    "symfony/messenger": "7.4.*",
    "symfony/monolog-bundle": "@stable",
    "symfony/postmark-mailer": "7.4.*",
    "symfony/property-access": "7.4.*",
    "symfony/requirements-checker": "@stable",
    "symfony/runtime": "7.4.*",
    "symfony/security-bundle": "7.4.*",
    "symfony/sendgrid-mailer": "7.4.*",
    "symfony/serializer": "7.4.*",
    "symfony/twig-bundle": "7.4.*",
    "symfony/validator": "7.4.*",
    "symfony/web-link": "7.4.*",
    "symfony/yaml": "7.4.*",
    "vaites/php-apache-tika": "^1.3",
    "webonyx/graphql-php": "^15.0"
  },
  "require-dev": {
    "infection/infection": "^0.32.0",
    "liip/test-fixtures-bundle": "3.8.0",
    "mockery/mockery": "@stable",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan": "^2.0",
    "phpstan/phpstan-mockery": "^2.0",
    "phpstan/phpstan-symfony": "^2.0",
    "phpunit/phpunit": "^12.0",
    "slevomat/coding-standard": "~8.0",
    "squizlabs/php_codesniffer": "@stable",
    "symfony/browser-kit": "7.4.*",
    "symfony/css-selector": "7.4.*",
    "symfony/debug-bundle": "7.4.*",
    "symfony/stopwatch": "7.4.*",
    "symfony/web-profiler-bundle": "7.4.*"
  },
  "config": {
    "platform": {
      "php": "8.4"
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "phpstan/extension-installer": true,
      "symfony/runtime": true,
      "infection/extension-installer": true,
      "php-http/discovery": false,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-mbstring": "*",
    "symfony/polyfill-php56": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*",
    "symfony/polyfill-php83": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "ilios-scripts": [
      "App\\Composer\\MigrateParameters::migrate",
      "App\\Composer\\MigrateCustomTemplates::migrate"
    ],
    "post-install-cmd": [
      "@auto-scripts",
      "@ilios-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts",
      "@ilios-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "7.4.*",
      "docker": false
    }
  }
}
