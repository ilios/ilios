{
  "name": "ilios/ilios",
  "license": "MIT",
  "type": "project",
  "description": "The \"Ilios Standard Edition\" distribution",
  "require": {
    "php": ">= 7.2",
    "ext-apcu": "*",
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-zlib": "*",
    "danielstjules/stringy": "^3.1",
    "doctrine/doctrine-fixtures-bundle": "^3.0",
    "dreamscapes/ldap-core": "^3.1",
    "easycorp/easy-log-handler": "^1.0",
    "elasticsearch/elasticsearch": "^6.1",
    "eluceo/ical": "^0.15.0",
    "exercise/htmlpurifier-bundle": "^2.0",
    "firebase/php-jwt": "@stable",
    "guzzlehttp/psr7": "^1.5",
    "ilios/mesh-parser": "^2.0",
    "league/flysystem": "^1.0",
    "liip/monitor-bundle": "^2.6",
    "nelmio/cors-bundle": "^1.5",
    "php-http/curl-client": "^1.7",
    "php-http/guzzle6-adapter": "^1.1",
    "php-http/httplug-bundle": "^1.3",
    "psr/log": "^1.0.0",
    "sensiolabs/security-checker": "*",
    "sentry/sentry": "2.0.0-beta-2",
    "swagger-api/swagger-ui": "^3.0",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "*",
    "symfony/console": "*",
    "symfony/flex": "^1.1",
    "symfony/framework-bundle": "*",
    "symfony/lock": "*",
    "symfony/monolog-bundle": "^3.1.0",
    "symfony/orm-pack": "1.0.5",
    "symfony/requirements-checker": "^1.1",
    "symfony/security-bundle": "*",
    "symfony/serializer-pack": "*",
    "symfony/swiftmailer-bundle": "^3.2",
    "symfony/templating": "*",
    "symfony/twig-bundle": "*",
    "symfony/validator": "*",
    "symfony/web-link": "*",
    "symfony/yaml": "*"
  },
  "require-dev": {
    "fzaninotto/faker": "@stable",
    "liip/functional-test-bundle": "~2.0@alpha",
    "mockery/mockery": "1.2.0",
    "squizlabs/php_codesniffer": "@stable",
    "symfony/debug-pack": "*",
    "symfony/dotenv": "*",
    "symfony/profiler-pack": "*",
    "symfony/test-pack": "*",
    "symfony/web-server-bundle": "*"
  },
  "config": {
    "platform": {
      "php": "7.2.0"
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
    },
    "ilios-scripts": [
      "App\\Composer\\MigrateParameters::migrate"
    ],
    "post-install-cmd": [
      "@auto-scripts",
      "@ilios-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts",
      "@ilios-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "4.2.*"
    }
  }
}
